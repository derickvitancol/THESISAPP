<Window x:Class="THESISAPP.DataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:THESISAPP"
        mc:Ignorable="d"
        Title="DataWindow" Height="450" Width="800">
    <Grid>
        <DataGrid  x:Name="datagridSensorData" HorizontalAlignment="Left" Height="266" Margin="37,35,0,0" VerticalAlignment="Top" Width="718"/>
        <Button x:Name="buttonClearData" Content="Clear All Data" HorizontalAlignment="Left" Margin="46,340,0,0" VerticalAlignment="Top" Width="135" RenderTransformOrigin="-1.253,0.3" Height="36" Click="ButtonClearData_Click"/>
        <Button x:Name="buttonReturn" Content="Return" HorizontalAlignment="Left" Margin="204,340,0,0" VerticalAlignment="Top" Width="135" Height="36" Click="ButtonReturn_Click"/>
        <Button x:Name="buttonRefresh" Content="Refresh" HorizontalAlignment="Left" Margin="360,340,0,0" VerticalAlignment="Top" Width="135" Height="36"  Click="ButtonRefresh_Click"/>

    </Grid>
</Window>
